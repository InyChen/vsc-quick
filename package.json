{
	"name": "vsc-quick",
	"displayName": "vsc-quick",
	"description": "quick way of fullstack development",
	"author": "chen jia lin",
	"version": "0.0.8",
	"publisher": "vsc-quick",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.quick",
				"title": "Quick快速开发"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "quickSideBar",
					"title": "Quick快速开发",
					"icon": "images/icon.svg"
				}
			]
		},
		"views": {
			"quickSideBar": [
				{
					"id": "quickSideBarView1",
					"name": "项目列表"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}